#include <stdio.h>
#include "main.h"

/**
* print_alphabet_x10 - Entry point
*
* Description: 'print a function that prints 10 times the alphabet, in lowercase'
*
* Return: Always 0 (Success)
*/

int print_alphabet(void)
{
char i;
char j;
for (i = 'a'; i <= 'z'; i++)
j = i;
for (j = i; j <= 5; j++)
putchar(j);
putchar('\n');
return (0);
}
